header{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  height: 175px;
  z-index: 200;
  h1{
    position: absolute;
    height: auto;
    width: 176px;
    top: 25px;
    left: 50%;
    margin-left: -88px;
    img{
      width: 100%;
      height: 100%;
    }
  }

  .back-bt{
    position: absolute;
    left: 50%;
    margin-left: -552px;
    margin-top: 60px;
    p{
      font-size: 16px;
      &:before{
        margin-left: -30px;
        content:" ";
        display: inline-block;
        position: absolute;
        height: 12px;
        width: 12px;
        border-color:#fff;
        border-top: 1px;
        border-left: 1px;
        border-top-style: solid;
        border-left-style: solid;
        @include transform(rotate(-45deg));
      }
    }
  }
}