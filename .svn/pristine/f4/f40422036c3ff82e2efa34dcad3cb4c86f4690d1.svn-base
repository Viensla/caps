    var FPS = function(){
        var performance = performance || false;
        var now = ( performance && performance.now ) ? performance.now.bind( performance ) : Date.now;
        var startTime = now(),
            prevTime = startTime;
        var frames = 0;
        var fps= 0;
        var globalFps = 0;
        var med=0;
        var tot = 0;

        return {
            begin : function(){
                startTime = now();
            },
            end : function(){
                var time = now();

                frames++;

                if ( time > prevTime + 1000 ) {

                    fps = Math.round( ( frames * 1000 ) / ( time - prevTime ) );
                    prevTime = time;
                    tot++;
                    globalFps = globalFps+fps;
                    med = globalFps/tot;
                    frames = 0;
                }
                return time;
            },
            update: function(){
                startTime = this.end();
                return {fps:fps, med:med};
            }
        }
    };



