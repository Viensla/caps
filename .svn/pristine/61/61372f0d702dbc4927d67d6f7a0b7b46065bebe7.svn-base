

.clear {
  clear: both;
}


#bg-test {
  position: fixed;
  z-index: 1;
  width: 100%;
  top:0
}

canvas {
  width: 100%;
  height: 100%;
}

body{
  width: 100%;
  //height: 100%;
  margin: 0;
  padding: 0;
  //overflow: hidden;
  font-family:$neutra;
  color: #fff;

  background: #eebc66;
}

h3{
  &.blck{
    color: #000;
    font-size: 25px;
  }
}

section.sc{
  position: relative;
  width: 100%;
  .sc-content{
    line-height: 1;
    display: inline-block;
    vertical-align: middle;
    h2{
      font-size: 25px;
      color: #fff;
      letter-spacing: 3px;
      text-align: center;
      @include neutraUp();
    }
  }
}


.bt{
  cursor:pointer;
  font-family: $neutra;
  text-align: center;
  &.wh{
    width: 230px;
    height: 52px;
    border: 1px solid #fbf4e9;
    p{
      height: 52px;
      line-height: 52px;
      width: 230px;
      font-size: 20px;
      text-align: center;
      text-transform: uppercase;
      color: #fff;
      font-weight: bold;
    }
  }
}



















