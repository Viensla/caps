// NEUTRA
$neutra : 'neutra';
@mixin neutraUp {font-family:$neutra; text-transform: uppercase }
.neutra {
  font-family:$neutra;
}


// PACIFICO
$pacifico : 'pacificoregular';
.pcfc {
  font-family:$pacifico ;
}

// ALEO
$aleoReg : 'aleoregular';
$aleoBoldIt : 'aleobolditalic';
$aleoBold : 'aleobold';
$aleoLightIt : 'aleolightitalic';
.aleo {
  font-family:$aleoReg;
}

.aleoB{
  font-family:$aleoBold;
}
