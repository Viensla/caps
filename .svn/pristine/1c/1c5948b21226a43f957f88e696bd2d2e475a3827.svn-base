

#game-area{
  width: 100%;
  height: 100%;
  z-index: 100;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
//  display: none;
}

#game-loader{
  position: absolute;
  z-index: 400;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: $brwBg;
  @include transition(.5s);

  &.fade{
    @include opacity(0);
  }
  .tiny-loader{
    width: 288px;
    height: 240px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -120px;
    margin-left: -144px;
    @include transition(.3s);
    &.fade{
      @include opacity(0);
    }
  }
}

#pause-menu{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 500;
  text-align: center;
  @include opacity(0);
  display: none;
  .pause-logo{
    margin-bottom: 25px;
    width: 302px;
    height: 179px;
  }
  p.lk{
    @include neutraUp;
    color: #fff;
    padding: 25px 0;
    cursor:pointer;
  }

  .lgt-close{
    margin-top: 20px;
  }

}

#versus-bar{
  position: absolute;
  left: 50%;
  margin-left: -215px;
  top: 73px;
  z-index: 100;
  width: 430px;
  .line{
    height: 3px;
    width: 100%;
    background: #000;
    position: absolute;
    left: 0;
    right: 0;
    top: 40px;
  }
  img{
    width: 60px;
    height: 60px;
    position: absolute;
    left: 50%;
    top: 13px;
    margin-left: -30px;
  }
  .los{
    position: absolute;
    width: 15px;
    height: 15px;
    background: #000;
    top: 34px;
    @include transform(rotate(45deg));
    &.pl{
      left: 0;
    }
    &.vl{
      right: 0;
    }
    &:after{
      content:" ";
      background: blue;
      position: absolute;
      width: 9px;
      height: 9px;
      margin: 3px;
    }
    &.chimey{
      &:after{
        background: $chimey;
      }
    }
    &.foster{
      &:after{
        background: $foster;
      }
    }
    &.lef{
      &:after{
        background: $lef;
      }
    }
    &.pelle{
      &:after{
        background: $pelle;
      }
    }
    &.chouffe{
      &:after{
        background: $chouffe;
      }
    }
    &.ptp{
      &:after{
        background: $ptp;
      }
    }
  }
}


.powerbar{
  width: 20px;
  height: 200px;
  position: absolute;
  overflow: hidden;
  @include border-radius(10px 10px 0 0);
  top: 0;
  left: 0;
  z-index: 30;
  display: none;
  @include mask(url('../images/mask/jauge.png'));
  @include mask-size(100% 100%);
  .bar{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 20px;
    height: 0%;
    @include border-radius(10px 10px 0 0);
    background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,1) 1%, rgba(255,255,255,1) 18%, rgba(255,255,255,0) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(1%,rgba(255,255,255,1)), color-stop(18%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 ); /* IE6-9 */
  }
}


#header{
  position: absolute;
  top: 0;
  height: 0px;
  background: #000;
  width: 100%;
  z-index: 50;
  h3{
    text-transform: uppercase;
    text-align: center;
    position: absolute;
    @include neutraUp();
    color: #000;
    font-size: 20px;
    top: 25px;
  }
  .pl-part{

    left: 50px;
    h3{
      left: 80px;
    }
    .capsstock{
      left: 8px;
    }
    .life-bloc{
      left: 16px;
    }
    .blason{
      left: 0px;
    }
  }

  .vl-part{
    right: 50px;
    h3{
      right: 80px;
    }
    .capsstock{
      right: 8px;
    }
    .life-bloc{
      right: 16px;
    }
    .blason{
      right: 0px;
    }
  }

  .pl-part, .vl-part{
    position: absolute;
    width: 200px;
    display: none;
    top: 80px;

    .blason{
      width: 66px;
      height: 66px;
      @include border-radius(50%);
      position: absolute;
      -webkit-background-size: 70px 70px;
      background-size: 70px 70px;
      background-position: -2px -2px;
    }
    &.chimey{
      .life{
        background: $chimey;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-chimais.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/chimais.png');
      }
    }
    &.foster{
      .life{
        background: $foster;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-faustaire.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/faustaire.png');
      }
    }
    &.lef{
      .life{
        background: $lef;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-lef.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/lef.png');
      }
    }
    &.pelle{
      .life{
        background: $pelle;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-pelle.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/pelle.png');
      }
    }
    &.chouffe{
      .life{
        background: $chouffe;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-shouffe.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/shouffe.png');
      }
    }
    &.ptp{
      .life{
        background: $ptp;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-ptp.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/ptp.png');
      }
    }
  }




  .capsstock{

    width: 50px;
    position: absolute;
    top: 250px;
    span{
      border-radius:50%;
      height: 50px;
      width: 50px;
      display: block;
      margin-top: 10px;
      cursor: pointer;
      background-size: 100% 100%;

    }
  }

  .life-bloc{
    position: absolute;
    top: 90px;
//    height: 144px;
//    width: 39px;
    height: 140px;
    width: 35px;
    background: url(../images/mask/bottle-mask-blc.png);
    .life-c{
