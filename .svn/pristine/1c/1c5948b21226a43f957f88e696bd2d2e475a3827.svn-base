.viewport{
  position: relative;
}


#txto-sd{
  position: absolute;
  right: 80px;
  bottom: 40px;
  z-index: 200;
  width: 220px;
  &:hover #sd-messages-box{
    height: 430px;
    @include opacity(1);
  }
  #bt-texto{
    width: 188px;
  }
  #sd-messages-box{
    @include transition(.3s);
    @include opacity(0);
    position: absolute;
    bottom: 0;
    left: 0;
    height: 0;
    width: 220px;
    overflow: hidden;
    background: $blue;

    &.disabled:hover{
      height: 0 !important;
      opacity:0 !important;
    }
    ul{
      width: 220px;
      background: $blue;
    }
    li{
      background: $blue;
      cursor:pointer;
      @include transition(.2s);
      &:hover {
        background: $blueDark;
      }
      &:last-child p{
        border-bottom: 0px;
      }
      p{
        color: #fff;
        font-size: 13px;
        line-height: 16px;
        border-bottom: 1px solid #8FEAE2;
        padding: 15px 5px;
        width: 190px;
        margin: auto;
      }
    }
  }
}

#rc-messages-box{
  position: absolute;
  z-index: 200;
  bottom: 170px;
  right: 400px;
  margin-right: -100px;
  background: #fff;
  color: #fff;
  border-radius:8px;
  padding: 10px;
//  display: none;
//  width: 200px;
//  height: 60px;
  width: 0;
  height: 0;
  line-height: 60px;
//  overflow: hidden;
  text-align: center;
  &:after{
    content:" ";
    width: 10px;
    height: 10px;
    @include transform(rotate(45deg));
    background: #fff;
    display: block;
    position: absolute;
    bottom: -5px;
    left: 50%;
    margin-left: -5px;
  }
  p{
    line-height: 16px;
    font-size: 13px;
    color: $blueDark;
    display: inline-block;
    vertical-align: middle;
  }

}


#caps-m, #start-m, #win-m, #lose-m, #perfect-m{
  width: 750px;
  height: 600px;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -300px;
  margin-left: -375px;
  overflow: hidden;
  display: none;

  background-position:0% 0%;
  pointer-events:none;
  &.capsIn1, &.capsIn2, &.capsIn3{
    display: block;
  }
}
#caps-m, #start-m{
  -webkit-background-size: 1000% 300%;
  background-size: 1000% 300%;
}
#win-m, #lose-m, #perfect-m{
  -webkit-background-size: 900% 300%;
  background-size: 900% 300%;
  &.capsIn1{
    @include animation(capsStep1 0.4s steps(8) 0s forwards);
  }
  &.capsIn2{
    @include animation(capsStep2 0.4s steps(8) 0s forwards);
  }
  &.capsIn3{
    @include animation(capsStep3 0.4s steps(8) 0s forwards);
  }
}

#caps-m{
  background-image: url('../images/sprites/sprite_caps.png');
}
#start-m{
  background-image: url('../images/sprites/sprite_start.png');
}
#win-m{
  background-image: url('../images/sprites/sprite_win.png');
}
#lose-m{
  background-image: url('../images/sprites/sprite_lose.png');
}
#perfect-m{
  background-image: url('../images/sprites/sprite_perfect.png');
}

.capsIn1{
  @include animation(capsStep1 0.4s steps(9) 0s forwards);
}
.capsIn2{
  @include animation(capsStep2 0.4s steps(9) 0s forwards);
}
.capsIn3{
  @include animation(capsStep3 0.4s steps(9) 0s forwards);
}

@include keyframes(capsStep1) {
  from {
    background-position : 0% 0%;
  }
  to {
    background-position : 100% 0%;
  }
}

@include keyframes(capsStep2) {
  from {
    background-position : 0% 50%;
  }
  to {
    background-position : 100% 50%;
  }
}


@include keyframes(capsStep3) {
  from {
    background-position : 0% 100%;
  }
  to {
    background-position : 100% 100%;
  }
}


@include keyframes(wave){
  0%{
    @include transform(skewY(5deg));
  }
  50%{
    @include transform(skewY(-5deg));
  }
  100%{
    @include transform(skewY(5deg));
  }
}