/* ############## FONTS ################### */
@font-face{
  font-family:"SEOptimist";
  src:url("../fonts/seoptimist-webfont.eot?#iefix");
  src:url("../fonts/seoptimist-webfont.eot?#iefix") format("eot"),
  url("../fonts/seoptimist-webfont.woff") format("woff"),
  url("../fonts/seoptimist-webfont.ttf") format("truetype"),
  url("../fonts/seoptimist-webfont.svg") format("svg");
  font-weight: normal;
}
@font-face{
  font-family:"LucidaGrande";
  src:url("../fonts/lucidagrande-webfont.eot?#iefix");
  src:url("../fonts/lucidagrande-webfont.eot?#iefix") format("eot"),
  url("../fonts/lucidagrande-webfont.woff") format("woff"),
  url("../fonts/lucidagrande-webfont.ttf") format("truetype"),
  url("../fonts/lucidagrande-webfont.svg") format("svg");
  font-weight: normal;
}
@font-face{
  font-family:"LucidaGrandeBold";
  src:url("../fonts/lucidagrande-bold-webfont.eot?#iefix");
  src:url("../fonts/lucidagrande-bold-webfont.eot?#iefix") format("eot"),
  url("../fonts/lucidagrande-bold-webfont.woff") format("woff"),
  url("../fonts/lucidagrande-bold-webfont.ttf") format("truetype"),
  url("../fonts/lucidagrande-bold-webfont.svg") format("svg");
  font-weight: normal;
}


.clear {
  clear: both;
}


#bg-test {
  position: fixed;
  z-index: 1;
  width: 100%;
  top:0
}

/* ############## VARIABLES ################### */

$lucidaGrandeBold : "LucidaGrandeBold";
$lucidaGrande : "LucidaGrande";
$seoptimist : "SEOptimist";

$flux : 900px;
$headerH : 60px;


canvas {
  width: 100%;
  height: 100%
}

body{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  overflow: hidden;
}

.powerbar{
  height: 100px;
  width: 10px;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 30;
  overflow: hidden;
  border-bottom: 1px solid #ddd;
  border-right: 1px solid #ddd;
  border-top: 1px solid #ddd;
  border-left: 1px solid #ddd;
  display: none;
  .bar{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 10px;
    height: 0%;
    background: #C1676A;

  }
}

#header{
  position: absolute;
  top: 0;
  height: 70px;
  background: #fedc2a;
  width: 100%;
  z-index: 50;
  h3{
    color: #fff;
    text-transform: uppercase;
    text-align: center;
  }
  .pl-part{
    position: absolute;
    left: 0;
    width: 200px;
  }
  .vl-part{
    position: absolute;
    right: 0;
    width: 200px;
  }
  .capsstock{
    display: none;
    span{
      background: #C1676A;
      border: 1px solid #000;
      border-radius:50%;
      height: 30px;
      width: 30px;
      display: inline-block;
      margin-left: 10px;
    }
  }

  .life-c{
    height: 120px;
    width: 30px;
    overflow: hidden;
    position: relative;
    border: 2px solid #000;

    .life{
      position: absolute;
      bottom: 0;
      height: 0%;
      width: 100%;
      @include transition(.5s);
      background: #fff;
    }
  }
}


.popin-drink-it{
  position: absolute;
  height: 500px;
  width: 500px;
  top: 50%;
  left: 50%;
  margin-left: -250px;
  margin-top: -250px;
  z-index: 100;
  .cache{
    border-radius:50%;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: absolute;
    overflow: hidden;
    border:2px solid  #C1676A;
    background: #fff;
    canvas{
    }
  }

}

#caps-m{
  font-size: 110px;
  text-transform: uppercase;
  text-align: center;
  position: absolute;
  width: 100%;
  top: 30%;
  display: none;
  color: #fff;
  z-index: 30;
}

#party{
  position: absolute;
  top: 50%;
  left: 50%;
  background: #fff;
  padding: 72px 30px;
  text-transform: uppercase;
  font-size: 30px;
  z-index: 20;
  cursor: pointer;
  margin-left: -87px;
  margin-top: -87px;
  border: 2px solid #000;
  border-radius:50%;
}

#winner{


}

#black_filter{
  background: #000;
  position: fixed;
  z-index: 10;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity:0.4;
}