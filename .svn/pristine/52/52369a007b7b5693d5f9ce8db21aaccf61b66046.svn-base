

#game-area{
  width: 100%;
  height: 100%;
  z-index: 100;
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
//  display: none;
}

#game-loader{
  position: absolute;
  z-index: 400;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: $brwBg;
  @include transition(.5s);
  &.fade{
    @include opacity(0);
  }
  .tiny-loader{
    width: 288px;
    height: 240px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -120px;
    margin-left: -144px;
    @include transition(.3s);
    &.fade{
      @include opacity(0);
    }
  }
}

#bt-menu{
  position: absolute;
  left: 80px;
  bottom: 40px;
  z-index: 200;
  width: 120px;
}
#pause-menu{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.35);
  z-index: 500;
  text-align: center;
  @include opacity(0);
  display: none;
  .pause-logo{
    margin-bottom: 25px;
    width: 302px;
    height: 179px;
  }
  p.lk{
    @include neutraUp;
    color: #fff;
    padding: 25px 0;
    cursor:pointer;
  }
  .lgt-close{
    margin-top: 20px;
  }

  #volets-c{
    position: relative;
    height: 240px;
    width: 600px;
  }
  .volet{
    @include transition(.5s);
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    &.hidden{
      @include opacity(0);
      @include transform(translateX(100%));
      &#home-pause{
        @include transform(translateX(-100%));
      }
    }
  }

  #about-pause{
    h3{
      margin: 30px 0;
    }
    .txt-c{
      line-height: 25px;
      span{
        color: $blue;
        font-size: 18px;
      }
    }
  }

  #tuto-pause{
    .back{
      line-height: 1;
      position: absolute;
      top: 50px;
      left: 50%;
      margin-left: -50px;
    }
    .bt{
      position: absolute;
      width: 220px;
      margin-left: -110px;
      left: 50%;
      bottom: 40px;
      line-height: 1;
    }
  }
  .help-c{
    position: absolute;
    line-height: 1;
    .arr{
      width: 44px;
      height: 42px;
      background-image: url('../images/pictos/tuto-arr-rg.png');
      -webkit-background-size: 100% 100%;
      background-size: 100% 100%;
      position: absolute;
      top: 0;
      left: 0;
    }
    .pcfc{
      font-size: 21px;
      text-align: left;
      span{
        font-size: 36px;
      }
    }
    .st{
      font-size: 16px;
      padding-left: 25px;
      text-align: left;
      line-height: 26px;

    }
  }
  .help-1{
    left: 140px;
    top: 450px;
    .rd{
      top: -128px;
      left: -98px;
    }
    .arr{
      top: -30px;
      left: -47px;
    }
  }
  .help-2{
    left: 50%;
    top: 22%;
    margin-left: 50px;
    .arr{
      background-image: url('../images/pictos/tuto-arr-lf.png');
      top: 24px;
      left: -50px;
    }

  }
  .help-3{
    left: 50%;
    margin-left: 70px;
    top: 68%;
    .arr{
      top: -20px;
      left: -60px;
    }
  }


  #quit-pause{
    .st{
      @include neutraUp;
      color: #fff;
      padding: 25px 0;
    }
    .bts{
      display: table;
      margin: 0 auto;
    }
    .bt{
      float: left;
      margin: 10px;
    }
  }
}

#reset-party-c{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.35);
  z-index: 500;
  text-align: center;
  @include opacity(0);
  display: none;
  .sc-content{
    position: relative;
  }
  &.winner{
    .typohand.win{
      display: inline-block;
    }
  }
  &.looser{
    .typohand.lose{
      display: inline-block;
    }
  }
  .typohand{
    margin-bottom: 25px;
    width: 302px;
    height: auto;
    @include transition(.3s);
    display: none;
  }
  .tiny-loader{
    width: 288px;
    height: 240px;
    position: absolute;
    top: 0;
    left: 50%;
    margin-left: -144px;
    @include transition(.3s);
    @include opacity(0);
  }
  h3{
    margin-bottom: 25px;
  }
  h4{
    @include neutraUp;
    line-height: 0px;
    height: 0;
    font-size: 20px;
  }
  .bt{
    margin: 25px auto 35px;
    @include transition(.3s);
  }

  &.loading{
    .typohand{
      @include opacity(0);
    }
    .tiny-loader{
      @include opacity(1);
    }
    .bt{
      @include transform(scale(0));
    }
  }


}


#versus-bar{
  position: absolute;
  left: 50%;
  margin-left: -215px;
  top: 73px;
  z-index: 100;
  width: 430px;
  #cursor{
    width: 0;
    height: 0;
    margin-left: -3px;

    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-top: 10px solid #000;
    position: absolute;
    top: 0;
    @include transition(.5s);
    @include animation(cursor 1.5s both infinite alternate linear);
    &.lf{
      left: 0;
    }
    &.rg{
      left: 415px;
    }

  }
  .line{
    height: 3px;
    width: 100%;
    background: #000;
    position: absolute;
    left: 0;
    right: 0;
    top: 40px;
  }
  img{
    width: 60px;
    height: 60px;
    position: absolute;
    left: 50%;
    top: 13px;
    margin-left: -30px;
  }
  .los{
    position: absolute;
    width: 15px;
    height: 15px;
    background: #000;
    top: 34px;
    @include transform(rotate(45deg));
    &.pl{
      left: 0;
    }
    &.vl{
      right: 0;
    }
    &:after{
      content:" ";
      background: blue;
      position: absolute;
      width: 9px;
      height: 9px;
      margin: 3px;
    }
    &.chimey{
      &:after{
        background: $chimey;
      }
    }
    &.foster{
      &:after{
        background: $foster;
      }
    }
    &.lef{
      &:after{
        background: $lef;
      }
    }
    &.pelle{
      &:after{
        background: $pelle;
      }
    }
    &.chouffe{
      &:after{
        background: $chouffe;
      }
    }
    &.ptp{
      &:after{
        background: $ptp;
      }
    }
  }
}


.powerbar{
  width: 20px;
  height: 200px;
  position: absolute;
  overflow: hidden;
  @include border-radius(10px 10px 0 0);
  top: 0;
  left: 0;
  z-index: 30;
  display: none;
  @include mask(url('../images/mask/jauge.png'));
  @include mask-size(100% 100%);
  .bar{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 20px;
    height: 0%;
    @include border-radius(10px 10px 0 0);
    background: -moz-linear-gradient(top,  rgba(255,255,255,1) 0%, rgba(255,255,255,1) 1%, rgba(255,255,255,1) 18%, rgba(255,255,255,0) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,1)), color-stop(1%,rgba(255,255,255,1)), color-stop(18%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* IE10+ */
    background: linear-gradient(to bottom,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 1%,rgba(255,255,255,1) 18%,rgba(255,255,255,0) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=0 ); /* IE6-9 */
  }
}


#header{
  position: absolute;
  top: 0;
  height: 0px;
  background: #000;
  width: 100%;
  z-index: 50;
  h3{
    text-transform: uppercase;
    text-align: center;
    position: absolute;
    @include neutraUp();
    color: #000;
    font-size: 20px;
    top: 25px;
  }
  .pl-part{

    left: 50px;
    h3{
      left: 80px;
    }
    .capsstock{
      left: 8px;
    }
    .life-bloc{
      left: 16px;
    }
    .blason{
      left: 0px;
    }

    .cap-tuto{
      position: absolute;
      left: 1px;
      top: 320px;
      @include transform(scale(0));
      .arr{
        height: 32px;
        width: 13px;
        background-image: url('../images/pictos/tiny-arrow.png');
        -webkit-background-size: 13px 32px;
        background-size: 13px 32px;
        margin-left: 24px;
        margin-bottom: 10px;
      }
      p{
        @include neutraUp;
        font-size: 10px;
        width: 67px;
        text-align: center;
      }
    }
  }

  .vl-part{
    right: 50px;
    h3{
      right: 80px;
    }
    .capsstock{
      right: 8px;
    }
    .life-bloc{
      right: 16px;
    }
    .blason{
      right: 0px;
    }
  }

  .pl-part, .vl-part{
    position: absolute;
    width: 200px;
    display: none;
    top: 80px;

    .blason{
      width: 66px;
      height: 66px;
      @include border-radius(50%);
      position: absolute;
      -webkit-background-size: 70px 70px;
      background-size: 70px 70px;
      background-position: -2px -2px;
    }
    &.chimey{
      .life{
        background: $chimey;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-chimais.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/chimais.png');
      }
    }
    &.foster{
      .life{
        background: $foster;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-faustaire.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/faustaire.png');
      }
    }
    &.lef{
      .life{
        background: $lef;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-lef.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/lef.png');
      }
    }
    &.pelle{
      .life{
        background: $pelle;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-pelle.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/pelle.png');
      }
    }
    &.chouffe{
      .life{
        background: $chouffe;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-shouffe.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/shouffe.png');
      }
    }
    &.ptp{
      .life{
        background: $ptp;
      }
      .blason{
        background-image: url('../images/beers/stickers/rounds/rd-ptp.png');
      }
      .capsstock span{
        background-image: url('../model/3D/caps_material/ptp.png');
      }
    }
  }




  .capsstock{

    width: 50px;
    position: absolute;
    top: 250px;
    span{
      border-radius:50%;
      height: 50px;
      width: 50px;
      display: block;
      margin-top: 10px;
      cursor: pointer;
      background-size: 100% 100%;

    }
  }

  .life-bloc{
    position: absolute;
    top: 90px;
//    height: 144px;
//    width: 39px;
    height: 140px;
    width: 35px;
    background: url(../images/mask/bottle-mask-blc.png);
    .life-c{
      height: 137px;
      width: 33px;
      top: 1px;
      left: 1px;
      overflow: hidden;
      position: absolute;
      @include mask(url(../images/mask/bottle-mask.png));
      svg{
        height: 138px;
        width: 33px;
        overflow: hidden;
      }
      .life{
        position: absolute;
        bottom: 0;
        height: 0%;
        padding-bottom: 30px;
        margin-bottom: -35px;
        @include opacity(0.8);
        width: 100%;
        @include transition(.5s);
        border-top: 4px solid #fff;
        @include animation(wave 2s both infinite alternate linear);
      }
    }
  }

}

.viewport{
  position: relative;
}

#you-turn{
  position: absolute;
  z-index: 100;
  left: 0px;
  top: 470px;
  width: 166px;
  opacity:0;
  height: 160px;
  background-image: url('../images/typos/you-turn.png');
  -webkit-background-size: 100% 100%;
  background-size: 100% 100%;
}

#txto-sd{
  position: absolute;
  right: 80px;
  bottom: 40px;
  z-index: 200;
  width: 220px;
  &:hover{
    #bt-texto{
      background: $blue;
    }
    #sd-messages-box{
      height: 430px;
      @include opacity(1);
    }
  }
  #bt-texto{
    width: 188px;
    @include transition(.3s);
  }
  #sd-messages-box{
    @include transition(.3s);
    @include opacity(0);
    position: absolute;
    bottom: 46px;
    left: 0;
    height: 0;
    width: 220px;
    overflow: hidden;
    background: $blue;

    &.disabled:hover{
      height: 0 !important;
      opacity:0 !important;
    }
    ul{
      width: 220px;
      background: $blue;
    }
    li{
      background: $blue;
      cursor:pointer;
      @include transition(.2s);
      &:hover {
        background: $blueDark;
      }
      &:last-child p{
        border-bottom: 0px;
      }
      p{
        color: #fff;
        font-size: 13px;
        line-height: 16px;
        border-bottom: 1px solid #8FEAE2;
        padding: 15px 5px;
        width: 190px;
        margin: auto;
      }
    }
  }
}

#rc-messages-box{
  position: absolute;
  z-index: 200;
  margin-left: -130px;
  margin-top: -170px;
  background: #fff;
  color: #fff;
  border-radius:8px;
  padding: 10px;
  width: 0;
  height: 0;
  line-height: 70px;
//  overflow: hidden;
  text-align: left;
  &:after{
    content:" ";
    width: 0px;
    height: 0px;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-top: 10px solid #fff;
    display: block;
    position: absolute;
    bottom: -10px;
    left: 50%;
    margin-left: -10px;
  }

  .center{
    line-height: 16px;
    display: inline-block;
    vertical-align: middle;
  }
  p{
    font-size: 13px;
    color: $blueDark;
    padding: 0;
    &:first-child{
      text-align: left;
      font-weight: bold;
    }
  }

}



#caps-m, #start-m, #win-m, #lose-m, #perfect-m{
  width: 750px;
  height: 600px;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -300px;
  margin-left: -375px;
  overflow: hidden;
  display: none;

  background-position:0% 0%;
  pointer-events:none;
  &.capsIn1, &.capsIn2, &.capsIn3{
    display: block;
  }
}
#caps-m, #start-m{
  -webkit-background-size: 1000% 300%;
  background-size: 1000% 300%;
}
#win-m, #lose-m, #perfect-m{
  -webkit-background-size: 900% 300%;
  background-size: 900% 300%;
  &.capsIn1{
    @include animation(capsStep1 0.4s steps(8) 0s forwards);
  }
  &.capsIn2{
    @include animation(capsStep2 0.4s steps(8) 0s forwards);
  }
  &.capsIn3{
    @include animation(capsStep3 0.4s steps(8) 0s forwards);
  }
}

#caps-m{
  background-image: url('../images/sprites/sprite_caps.png');
}
#start-m{
  background-image: url('../images/sprites/sprite_start.png');
}
#win-m{
  background-image: url('../images/sprites/sprite_win.png');
}
#lose-m{
  background-image: url('../images/sprites/sprite_lose.png');
}
#perfect-m{
  background-image: url('../images/sprites/sprite_perfect.png');
}

.capsIn1{
  @include animation(capsStep1 0.4s steps(9) 0s forwards);
}
.capsIn2{
  @include animation(capsStep2 0.4s steps(9) 0s forwards);
}
.capsIn3{
  @include animation(capsStep3 0.4s steps(9) 0s forwards);
}

@include keyframes(capsStep1) {
  from {
    background-position : 0% 0%;
  }
  to {
    background-position : 100% 0%;
  }
}

@include keyframes(capsStep2) {
  from {
    background-position : 0% 50%;
  }
  to {
    background-position : 100% 50%;
  }
}


@include keyframes(capsStep3) {
  from {
    background-position : 0% 100%;
  }
  to {
    background-position : 100% 100%;
  }
}


@include keyframes(wave){
  0%{
    @include transform(skewY(5deg));
  }
  50%{
    @include transform(skewY(-5deg));
  }
  100%{
    @include transform(skewY(5deg));
  }
}



@include keyframes(cursor){
  0%{
    @include transform(translateY(0));
  }
  50%{
    @include transform(translateY(0));
  }
  75%{
    @include transform(translateY(3px));
  }
  100%{
    @include transform(translateY(0));
  }
}